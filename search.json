---
layout: none
---
[
  {% for document in site.documents %}
    {
      "title"    : "{{ document.title | escape }}",
      "tags"     : "{{ document.tags | join: ', ' }}",
      "url"      : "{{ site.baseurl }}{{ document.url }}",
      "date"     : "{{ document.date }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
